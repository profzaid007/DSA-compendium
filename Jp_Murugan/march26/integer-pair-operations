import java.util.*;

class Main{
	public static void main(String[]args){
		Scanner sc = new Scanner(System.in);
		int[] arr1 = new int[2];
		arr1[0] = sc.nextInt();
		arr1[1] = sc.nextInt();
		
		int[] arr2 = new int[2];
		arr2[0] = sc.nextInt();
		arr2[1] = sc.nextInt();
		boolean flag = true;
		while(arr1[0] != arr2[0] && arr2[1] !=arr1[1]){
			if(arr1[0]>arr2[0] || arr1[1]>arr2[1]){
				flag = false;
				break;
			}
			arr1[0]+=arr1
		}
		
		System.out.println(flag);
	}
}